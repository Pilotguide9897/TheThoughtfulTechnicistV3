<div class="userBlogPosts">
  {{#each blogpostData}}
    <br>
    <div class="userPost bg-lightblue" id="editablePost" data-post-id="{{id}}">
      <h2 class="post-title-text">{{title}}</h2>
      <p>Posted on <span class="bold-text">{{format_date createdAt}}.</span> </p>
      <button type="button" class="btn btn-secondary dashboard-viewIndividualUserPost">View Post</button>
    </div>
  {{/each}}
</div>
<br>
<div>
  <button type="button" class="btn btn-success" id="dashboard-createPost">Create Post</button>
</div>



